{
  "cli": {
    "version": ">= 16.15.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal"
    },
    "production": {
      "android": {
        "gradleCommand": ":app:assembleRelease",
        "buildType": "apk",
        "image": "latest",
        "node": "20"
      },
      "env": {
        "EXPO_ANDROID_GRADLE_PLUGIN_VERSION": "8.5.2",
        "EXPO_ANDROID_GRADLE_VERSION": "8.9"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
